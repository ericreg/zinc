fn main() {

    // variable assignment
    x = 1
    print("x: {x}")

    x = 2
    print("x: {x}")

    x = 2.17
    print("x: {x}")
    x = 3.14

    print("x: {x}")

    x = "hello"
    print("x: {x}")

    x = "world"
    print("x: {x}")

    x = true
    print("x: {x}")
    
    x = false
    print("x: {x}")
}
