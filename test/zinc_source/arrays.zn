fn main() {

    // create an array
    a = [1, 2, 3]

    // print elements
    print("{a[0]}")
    print("{a[1]}")
    print("{a[2]}")


    // gets promoted to a vector if we use push
    b = []
    b.push(10)
    b.push(20)
    b.push(30)

    // print elements
    print("{b[0]}")
    print("{b[1]}")
    print("{b[2]}")
}
